n = int(input())
result = [[[print(n, end = " года") if n % 100 <= 4 and n % 100 != 0 else print(n, end = " лет")] if n % 100 != 1 else print(n, end = " год")] if n % 100 < 20 else
          [[print(n, end = " года") if n % 10 <= 4 and n % 10 != 0 else print(n, end = " лет")] if n % 10 != 1 else print(n, end = " год")]]
